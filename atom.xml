<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

 <title>Pedro von Hertwig Batista</title>
 <link href="http://pedrovhb.com/atom.xml" rel="self"/>
 <link href="http://pedrovhb.com/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://pedrovhb.com/</id>
 <author>
   <name>Pedro von Hertwig Batista</name>
   <email>pedrovhb@gmail.com</email>
 </author>

 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://pedrovhb.com{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://pedrovhb.com{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}

</feed>
